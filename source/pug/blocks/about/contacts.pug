.contacts
    .contacts__title
        +subtitle('contacts', 'Контакты')
    ul.contacts__list
        each item in contacts
            li.contacts__item
                a.contacts__link(href=`${item.link}` target="_blank")
                    svg.contacts__img
                        use(xlink:href=`./assets/img/sprite.svg#${item.img}`)
                    .contacts__text #{item.text}
